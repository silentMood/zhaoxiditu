{
  "name": "app-react-mobx-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "mobx": "^5.14.2",
    "mobx-react": "^6.1.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "antd-mobile": "^2.3.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "preanalyze": "react-app-rewired build --stats",
    "analyze": "npx webpack-bundle-analyzer build/bundle-stats.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "npm run build && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "babel-plugin-import": "^1.13.0",
    "commitlint": "^8.3.5",
    "customize-cra": "^0.8.0",
    "husky": "^4.2.1",
    "node-sass": "^4.13.1",
    "postcss-normalize": "^8.0.1",
    "postcss-px2rem": "^0.3.0",
    "react-app-rewired": "^2.1.4",
    "sass-loader": "^8.0.2"
  }
